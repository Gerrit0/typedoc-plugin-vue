{
  "name": "typedoc-plugin-vue",
  "description": "Improves Vue defineComponent and Pinia defineStore display in TypeDoc documentation",
  "type": "module",
  "version": "1.2.0",
  "main": "dist/plugin.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "prettier --check .",
    "test": "vitest run",
    "docs": "typedoc --plugin ./dist/plugin.js src/testdata/component.ts"
  },
  "devDependencies": {
    "outdent": "^0.8.0",
    "pinia": "^2.1.7",
    "prettier": "^3.3.2",
    "typedoc": "^0.26.0",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0",
    "vue": "^3.4.30"
  },
  "keywords": [
    "typedoc-plugin",
    "vue",
    "pinia"
  ],
  "peerDependencies": {
    "typedoc": "0.25.x || 0.26.x"
  },
  "files": [
    "dist/plugin.js"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/Gerrit0/typedoc-plugin-vue.git"
  }
}
